<?xml version="1.0" encoding="UTF-8"?>
<dispatch-entries>

  <!-- Serve landing page from site module -->
  <dispatch>
    <url>*/</url>
    <module>site</module>
  </dispatch>

  <!-- Send old Wordpress URLs to site module for redirect -->
  <dispatch>
    <url>about.activityinfo.org/</url>
    <module>site</module>
  </dispatch>

  <!-- Serve about and blog resources from site -->
  <dispatch>
    <url>*/about/*</url>
    <module>site</module>
  </dispatch>
  <dispatch>
    <url>*/blog/*</url>
    <module>site</module>
  </dispatch>

</dispatch-entries>