<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:chrome='urn:import:org.activityinfo.ui.client.chrome'
        >

  <ui:with field="icons" type="org.activityinfo.ui.client.style.Icons"/>

  <g:HTMLPanel>

    <chrome:PageHeader ui:field="pageHeader"/>

    <div class="contentpanel">

      <ul class="nav nav-tabs">
        <li class="active" ui:field="tableTabContainer">
          <a ui:field="tableTab">
            <strong>Table</strong>
          </a>
        </li>
        <li ui:field="designTabContainer">
          <a ui:field="designTab">
            <strong>Design</strong>
          </a>
        </li>
      </ul>
      <div class="tab-content">
        <div id="popular" class="tab-pane active">
          <g:Widget ui:field="tabWidget"/>
        </div>
      </div>
    </div>
  </g:HTMLPanel>
</ui:UiBinder>